{
    "name": "llcu/us-states",
    "description": "A PHP package providing US states data with static method access",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "LLC University",
        "email": "tech@llcuniversity.com"
      }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "pestphp/pest": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "LLCU\\UsStates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LLCU\\UsStates\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
